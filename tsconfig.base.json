{
  "compilerOptions": {
    "target": "ES2021",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "allowJs": false,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "paths": {
      "@ui/*": [
        "packages/ui/src/*"
      ],
      "@shared/*": [
        "packages/shared/src/*"
      ],
      "@supaclient/*": [
        "packages/supabase/src/*"
      ]
    }
  }
}